//merge.lc
//compare the head of each list
//whichever one is less becomes the head of the new list
//remove head from the lesser list
//repeat until no more elements

let rec le = \x.\y.
	if x=0 then
		S 0
	else
		if y=0 then
			0
		else
			if x = y then
				S 0
			else
				le (minus_one x) (minus_one y) in 
let rec merge = \x.\y.
	if le (hd x) (hd y)=S 0 then
		if tl x = # then 
			(hd x) : (y)
		else
			(hd x) : (merge (tl x) y)
	else
		if tl y = # then 
			(hd y) : (x)
		else
			(hd y) : (merge (tl y) x) in

merge (0 : S S 0 : S S S S 0 : #) (S 0 : S S S 0 : #) 
