val newCList = \e.
    let val a = new [] in
    a := [e,a];
    a;;

-- complete the following functions

val next = \a.
	case !a of {
		[e', a'] -> a'
	};;

val get = \e. 
	case !e of {
    [e,a] -> e
	};;

val insert = \e. \a.
	let val x = new [] in
    x := [e, next a];
    a := [head !a, x];
    x;;
	
-- val delete = \a.

val update = \e. \a.
	a := [e, a];;